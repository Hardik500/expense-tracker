generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type BankFieldsMapFieldMap {
  field String
  label String
}

model bank_fields_map {
  id        String                  @id @default(auto()) @map("_id") @db.ObjectId
  bank_name String
  field_map BankFieldsMapFieldMap[]
}

model bank_statement {
  id        String                  @id @default(auto()) @map("_id") @db.ObjectId
  bank_name String
  description String
  debit Float
  credit Float
  balance Float
  category String
  date DateTime
}